f = print_copy.sort_values(by='similarity', ascending=False)\n","        t_df = pd.concat([t_df,df])\n","    \n","    # 작품끼리 유사도 더하기\n","    t_df = t_df.groupby(['title','writer','illustrator', 'day', 'genre','story','platform'],as_index=False).sum()\n","    \n","    \n","    return t_df.sort_values(by='similarity',ascending=False).iloc[len(title):rank+len(title)+1,:]"],"execution_count":null,"outputs":[]},{"cell_type":"markdown","metadata":{"id":"NZRtK8uROhIw"},"source":["### 중복처리 시현"]},{"cell_type":"code","metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"_pLNsd9XyWsH","executionInfo":{"status":"ok","timestamp":1632306705334,"user_tz":-540,"elapsed":26,"user":{"displayName":"이우재","photoUrl":"https://lh3.googleusercontent.com/a/default-user=s64","userId":"15709195273175322901"}},"outputId":"1e1a4061-a067-42c8-b714-f76430493bd9"},"source":["idx = indices['트리거']\n","type(idx)"],"execution_count":null,"outputs":[{"output_type":"execute_result","data":{"text/plain":["pandas.core.series.Series"]},"metadata":{},"execution_count":7}]},{"cell_type":"code","metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"7gZFTZAWUIh1","executionInfo":{"status":"ok","timestamp":1632306705336,"user_tz":-540,"elapsed":24,"user":{"displayName":"이우재","photoUrl":"https://lh3.googleusercontent.com/a/default-user=s64","userId":"15709195273175322901"}},"outputId":"96c1ebdb-21f5-49a7-8b06-ffe0e79183b0"},"source":["idx.size"],"execution_count":null,"outputs":[{"output_type":"execute_result","data":{"text/plain":["2"]},"metadata":{},"execution_count":8}]},{"cell_type":"code","metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"lE1iCqQ5WKuG","executionInfo":{"status":"ok","timestamp":1632306705338,"user_tz":-540,"elapsed":22,"user":{"displayName":"이우재","photoUrl":"https://lh3.googleusercontent.com/a/default-user=s64","userId":"15709195273175322901"}},"outputId":"8751a6f7-4e24-42be-f960-1b61cf4afe30"},"source":["idx"],"execution_count":null,"outputs":[{"output_ty